{
	"name": "iap-click-hourly-trigger",
	"properties": {
		"annotations": [],
		"runtimeState": "Stopped",
		"pipeline": {
			"pipelineReference": {
				"referenceName": "iap-reporting-click-hourly",
				"type": "PipelineReference"
			},
			"parameters": {
				"inputClick": "@concat('wasbs://iap-streams@iapblob.blob.core.windows.net/iapstreams/iapclick/*', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy/MM/dd/HH/*/'))",
				"outputClick": "@concat('wasbs://iap-reporting-data@iapblob.blob.core.windows.net/iapclick', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy-MM-dd-HH'))"
			}
		},
		"type": "TumblingWindowTrigger",
		"typeProperties": {
			"frequency": "Hour",
			"interval": 1,
			"startTime": "2019-01-13T09:00:00Z",
			"delay": "00:00:00",
			"maxConcurrency": 50,
			"retryPolicy": {
				"intervalInSeconds": 30
			}
		}
	}
}
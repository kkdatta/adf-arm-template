{
	"name": "iap-billing-hourly-trigger",
	"properties": {
		"annotations": [],
		"runtimeState": "Stopped",
		"pipeline": {
			"pipelineReference": {
				"referenceName": "iap-reporting-billing-hourly",
				"type": "PipelineReference"
			},
			"parameters": {
				"inputBilling": "@concat('wasbs://iap-streams@iapblob.blob.core.windows.net/iapstreams/iapbilling/*', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy/MM/dd/HH/*/'))",
				"outputBilling": "@concat('wasbs://iap-reporting-data@iapblob.blob.core.windows.net/iapbilling', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy-MM-dd-HH'))"
			}
		},
		"type": "TumblingWindowTrigger",
		"typeProperties": {
			"frequency": "Hour",
			"interval": 1,
			"startTime": "2019-01-13T08:00:00Z",
			"delay": "00:00:00",
			"maxConcurrency": 5,
			"retryPolicy": {
				"intervalInSeconds": 300
			}
		}
	}
}
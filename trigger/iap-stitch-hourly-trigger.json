{
	"name": "iap-stitch-hourly-trigger",
	"properties": {
		"annotations": [],
		"runtimeState": "Stopped",
		"pipeline": {
			"pipelineReference": {
				"referenceName": "iap-reporting-summary-hourly",
				"type": "PipelineReference"
			},
			"parameters": {
				"inputClick": "@concat('wasbs://iap-streams@iapblob.blob.core.windows.net/iapstreams/iapclick/*', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy/MM/dd/HH/*/'))",
				"inputBilling": "@concat('wasbs://iap-streams@iapblob.blob.core.windows.net/iapstreams/iapbilling/*', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy/MM/dd/HH/*/'))",
				"processTime": "@formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy-MM-dd HH:00:00')",
				"outputData": "@concat('wasbs://iap-reporting-data@iapblob.blob.core.windows.net/iapstitch', '/', formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy-MM-dd-HH'))",
				"outputLocation": "@concat('iap-reporting-data/iapstitch/',formatDateTime(addhours(triggerOutputs().windowStartTime, 0), 'yyyy-MM-dd-HH'))"
			}
		},
		"type": "TumblingWindowTrigger",
		"typeProperties": {
			"frequency": "Hour",
			"interval": 1,
			"startTime": "2019-01-14T05:00:00Z",
			"delay": "00:00:00",
			"maxConcurrency": 50,
			"retryPolicy": {
				"intervalInSeconds": 30
			}
		}
	}
}